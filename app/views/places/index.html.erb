<p id="notice"><%= notice %></p>

<h1>Listing Places</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Url name</th>
      <th>State</th>
      <th>Issues fetched at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.name %></td>
        <td><%= place.url_name %></td>
        <td><%= place.state %></td>
        <td><%= place.issues_fetched_at %></td>
        <td><%= link_to 'Show', place %></td>
      </tr>
    <% end %>
  </tbody>
</table>
